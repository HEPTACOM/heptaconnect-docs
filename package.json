{
    "name": "heptaconnect-docs",
    "version": "1.0.0",
    "main": "index.js",
    "scripts": {
        "dev": "npm run development",
        "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "watch": "npm run development -- --watch",
        "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
        "prod": "npm run production",
        "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "rss": "cross-env NODE_ENV=production node rss.js",
        "mkdocs-pdf": "cross-env NODE_ENV=production node pdf.js",
        "html-minify": "cross-env NODE_ENV=production node node_modules/.bin/html-minifier --remove-tag-whitespace --collapse-whitespace --preserve-line-breaks --file-ext html --input-dir site --output-dir site"
    },
    "license": "MIT",
    "devDependencies": {
        "cross-env": "^7.0.2",
        "docsify": "^4.11.6",
        "docsify-copy-code": "^2.1.0",
        "docsify-edit-on-github": "^1.0.3",
        "docsify-page-toc": "^1.0.4",
        "docsify-pagination": "^2.6.0",
        "feed": "^4.2.2",
        "html-minifier": "^4.0.0",
        "js-yaml": "^4.1.0",
        "laravel-mix": "^5.0.5",
        "laravel-mix-purgecss": "^6.0.0",
        "markdown-it": "^12.0.4",
        "resolve-url-loader": "^3.1.0",
        "sass": "^1.26.10",
        "sass-loader": "^8.0.2",
        "vue-template-compiler": "^2.6.12"
    }
}
